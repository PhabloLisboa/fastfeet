{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "src/dist",
    "baseUrl": "src/",
    "paths": {
        "*": [
            "node_modules/*",
            "src/types/*"
            ]
        }
    },
  "exclude": ["**/*.test.ts"],
  "experimentalDecorators": true,
  "emitDecoratorMetadata": true
}
